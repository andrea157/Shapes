<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShapeLayout">
        <attr name="drawableId" format="reference" />
    </declare-styleable>

    <declare-styleable name="ArchLayout">
        <attr name="arch_height" format="dimension" />
        <attr name="arch_position" format="enum">
            <enum name="bottom" value="1" />
            <enum name="top" value="2" />
            <enum name="left" value="3" />
            <enum name="right" value="4" />
        </attr>
    </declare-styleable>

</resources>